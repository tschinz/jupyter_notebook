   "source": [
    "{\n",
    "  \"runmenu:restart-and-run-all\": {\n",
    "    \"command\": \"runmenu:restart-and-run-all\",\n",
    "    \"keys\": [\n",
    "      \"Ctrl Shift Alt Enter\"\n",
