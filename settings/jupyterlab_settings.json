{
  "runmenu:restart-and-run-all": {
    "command": "runmenu:restart-and-run-all",
    "keys": [
      "Ctrl Shift Alt Enter"
    ],
    "selector": "[data-jp-code-runner]",
    "title": "Restart Kernel and Run All",
    "category": "Run Menu"
  },
  "runmenu:run-all": {
    "command": "runmenu:run-all",
    "keys": [
      "Ctrl Shift Enter"
    ],
    "selector": "[data-jp-code-runner]",
    "title": "Run All",
    "category": "Run Menu"
  }
}
